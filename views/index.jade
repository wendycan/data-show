extends layout

block content
  div.sidebar.left
    ul
      li#areas 领域
      li#locations 地域
      li#times 融资次数
      li#sum 融资金额
  div#content.content.right
  script(type='text/template', id='t-areas')
    div.columns-2
      div#areas-0
      div#areas-1
      div#areas-2
      div#areas-3
      div#areas-4
  script(type='text/template', id='t-locations')
    div.map#locations-0
  script(type='text/template', id='t-times')
  script(type='text/template', id='t-sum')
